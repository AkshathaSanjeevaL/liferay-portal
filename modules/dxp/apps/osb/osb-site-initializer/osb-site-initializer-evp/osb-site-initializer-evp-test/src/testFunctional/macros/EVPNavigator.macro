definition {

	macro assertActiveBreadcrumb {
		AssertElementPresent(
			locator1 = "Breadcrumb#BREADCRUMB_ENTRY_ACTIVE_ENTRY",
			value1 = ${activeItem});
	}

	macro goToNavItem {
		Click(
			key_navItem = ${navItem},
			locator1 = "NavBar#NAV_ITEM_REDIRECTION");

		WaitForPageLoad();
	}

	macro navItemNotPresent {
		AssertElementNotPresent(
			key_navItem = ${navItem},
			locator1 = "NavBar#NAV_ITEM_REDIRECTION");
	}

}