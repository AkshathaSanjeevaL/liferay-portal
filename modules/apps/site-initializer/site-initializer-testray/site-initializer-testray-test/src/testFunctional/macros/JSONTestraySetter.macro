definition {

	@summary = "Get the Id of build."
	macro getBuildId {
		var buildId = JSONTestrayAPI._getObjectId(
			object = "builds",
			objectName = ${buildName});

		return ${buildId};
	}

	@summary = "Get the Id of case."
	macro getCaseId {
		var caseId = JSONTestrayAPI._getObjectId(
			object = "cases",
			objectName = ${caseName});

		return ${caseId};
	}

	@summary = "Get the Id of case result."
	macro getCaseResultId {
		var caseResultId = JSONTestrayAPI._getObjectId(
			object = "caseresults",
			objectName = ${caseResultName});

		return ${caseResultId};
	}

	@summary = "Get the Id of case type."
	macro getCaseTypeId {
		var caseTypeId = JSONTestrayAPI._getObjectId(
			object = "casetypes",
			objectName = ${caseType});

		return ${caseTypeId};
	}

	@summary = "Get the Id of component"
	macro getComponentId {
		var componentId = JSONTestrayAPI._getObjectId(
			object = "components",
