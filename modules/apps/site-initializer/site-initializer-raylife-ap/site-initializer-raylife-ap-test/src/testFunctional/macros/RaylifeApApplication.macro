definition{
    macro modalFlow{
             Click(
				locator1="Button#ANY",
				key_text="Application");
                
             Click(
				locator1="RaylifeApplicationModal#MODAL_OPTION",
				key_text="${insurance}");

			Click(
				locator1="Button#ANY",
				key_text="Next");
			
		if (isSet(button)) {
            Click(
				locator1="Button#ANY",
				key_text="${button}");
        }

        if (isSet(product)) {
            Click(
				locator1="RaylifeApplicationModal#MODAL_OPTION",
				key_text="${product}");

            Click(
				locator1="Button#ANY",
				key_text="Next");
        }
    }
    macro modalOption {
        Click(
				locator1="RaylifeApplicationModal#MODAL_OPTION",
				key_text="${option}");
    }
}